      GtkEventBox(header):
        GtkLabel(): '<b>The following items don&apos;t have enough stock to confirm.</b>'
      GtkEventBox(main, expand=True, fill=True, padding=6):
        ObjectList(_klist):
          column: title='Product', expand
          column: title='Ordered'
          column: title='Stock'
          row: 'desc', Decimal('1'), Decimal('0')
