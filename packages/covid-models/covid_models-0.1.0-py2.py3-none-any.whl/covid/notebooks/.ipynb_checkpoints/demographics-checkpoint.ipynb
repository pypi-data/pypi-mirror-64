   "source": [
    "from covid import data\n",
    "\n",
    "df = data.cia_factbook('age distribution')\n",
    "data = df[(df.region == 'Brazil') & (df.ref_date == 2020)]\n",
    "data = data.loc[:, '0-4':].T\n",
    "data.index.name = 'age'\n",
    "data = data.iloc[:, 0].apply(int)\n",
    "data"
   ]
