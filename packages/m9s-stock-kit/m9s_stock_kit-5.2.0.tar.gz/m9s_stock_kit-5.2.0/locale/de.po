msgctxt "error:product.product:"
msgid ""
"The product \"%s\" is configured as a kit with \"Stock Depends on Components\".\n"
"Its template must be configured as a Service or Consumable."
msgstr ""
"Die Variante \"%s\" ist als Kit mit  \"Lagerbestand basiert auf Komponenten\" konfiguriert.\n"
"Der Artikel muss als Dienstleistung oder Verbrauchsmaterial konfiguriert sein."

msgctxt "field:product.product,stock_depends_on_kit_components:"
msgid "Stock Depends on Components"
msgstr "Lagerbestand basiert auf Komponenten"

msgctxt "help:product.product,stock_depends_on_kit_components:"
msgid ""
